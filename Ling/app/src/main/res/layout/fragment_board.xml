<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".board.BoardFragment"
    android:background="@drawable/main_back_gradient_animation">
    <com.example.ling.board.KghNav
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:appbarColor="#F2BED1"
        app:HamMenuIconTintColor="#fff"
        app:appbarTitleTextColor="#fff"
        android:outlineAmbientShadowColor="#F0D3D8"
        app:HamMenuItemSemiTransparentColor="#B7D6F5"
        app:primaryMenuItemTextColor="#F0D3D8"
        app:secondaryMenuItemTextColor="#B7D6F5"
        android:id="@+id/navigationDrawer">
        <FrameLayout
            android:background="@color/black"
            android:layout_width="match_parent"
            android:layout_height="1dp"/>
    <FrameLayout
        android:id="@+id/container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    </com.example.ling.board.KghNav>
</androidx.constraintlayout.widget.ConstraintLayout>